int PARAM = 5;

int fibo(int n)
{
    int i = 0;
    int j = 1;
    int temp = 0;
    int k = 0;
    while (k < n)
    {
        temp = i;
        i = j;
        j = j + temp;
        k = k + 1;
    }
    return i;
}

int fact(int n)
{
    if (n < 2)
    {
        return 1;
    }
    else
    {
        return n * fact(n + -1);
    }
}

void main()
{
    putchar(fact(PARAM));
}